    <!-- Header  -->

    <div class="hero">
      <div class="hero-content text-center text-neutral-content">
        <div class="max-w-md">
          <h1 class="mb-5 mt-8 text-4xl font-white font-semibold text-white">
            Contact
          </h1>
          <p class="mb-5 text-1xl text-white">
            Fill out the form below to get in contact with me!
          </p>
        </div>
      </div>
    </div>

    <!-- Contact Form -->

    <div class="pt-2 flex flex-wrap justify-center mb-8">
      <div class="flex card w-96" style="background-color: #111111">
        <div class="card-body">
          <div class="form-control w-full max-w-xs">
            <label class="label">
              <span class="label-text">What is your name?</span>
            </label>
            <input
              type="text"
              id="name"
              placeholder="John Doe"
              class="input input-bordered w-full max-w-xs"
              style="background-color: #222222"
            />
          </div>
          <div class="form-control w-full max-w-xs">
            <label class="label">
              <span class="label-text">What is your email?</span>
            </label>
            <input
              type="email"
              id="email"
              placeholder="hi@itsalexander.dev"
              class="input input-bordered w-full max-w-xs"
              style="background-color: #222222"
            />
          </div>
          <div class="form-control w-full max-w-xs mb-2">
            <label class="label">
              <span class="label-text">What do you have to say?</span>
            </label>
            <textarea
              id="message"
              class="textarea textarea-bordered h-24"
              placeholder="I love your website!"
              style="background-color: #222222"
            ></textarea>
          </div>
          <div id="submit" class="card-actions">
            <button
              id="submit-button"
              class="btn btn-block text-white"
              style="background-color: #333333"
            >
              Submit
            </button>
          </div>
        </div>
      </div>
    </div>

    <footer
      class="sticky top-[100vh] p-4 shadow md:flex md:items-center sm:justify-between md:justify-between md:p-6 flex flex-wrap"
      style="background-color: #111111"
